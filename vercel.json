{
  "version": 2,
  "name": "suggestly-elite-business-platform",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "pages/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/home",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/suggestly-elite-ultimate-platform.html"
    },
    {
      "src": "/analytics",
      "dest": "/suggestly-elite-ultimate-platform.html"
    },
    {
      "src": "/quantum",
      "dest": "/pages/quantum-status.html"
    },
    {
      "src": "/live-data",
      "dest": "/live-data-stream.html"
    },
    {
      "src": "/deploy",
      "dest": "/live-deployment-setup.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
