{
  "version": 2,
  "name": "suggestly-elite-business-platform",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/suggestly-elite-ultimate-platform.html"
    },
    {
      "source": "/analytics",
      "destination": "/suggestly-elite-ultimate-platform.html"
    },
    {
      "source": "/quantum",
      "destination": "/pages/quantum-status.html"
    },
    {
      "source": "/live-data",
      "destination": "/live-data-stream.html"
    },
    {
      "source": "/deploy",
      "destination": "/live-deployment-setup.html"
    }
  ],
  "headers": [
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
