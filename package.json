  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "ajv": "^8.12.0",
    "express": "^5.1.0",
    "prettier": "^3.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "npm run build && node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md}",
    "type-check": "tsc --noEmit",
    "security-check": "npm audit",
    "security-fix": "npm audit fix",
    "security-fix-force": "npm audit fix --force",
    "build:clean": "rm -rf build",
    "optimize": "node performance-optimizer.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "optimize:ultra": "node ultra-premium-optimizer.js",
    "dev:ultra": "set NODE_OPTIONS=--max-old-space-size=4096 && concurrently \"npm run start\" \"npm run server\"",
    "build:ultra": "set NODE_OPTIONS=--max-old-space-size=4096 && npm run build",
    "test:ultra": "set NODE_OPTIONS=--max-old-space-size=4096 && npm test -- --coverage --watchAll=false",
    "performance:ultra": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-ultra-report.json",
    "benchmark:ultra": "autocannon -c 200 -d 60 http://localhost:3000",
    "analyze:ultra": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "security:ultra": "npm audit && snyk test && retire",
    "clean:ultra": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json && if exist build rmdir /s /q build && if exist dist rmdir /s /q dist",
    "install:ultra": "npm run clean:ultra && npm install --prefer-offline --no-audit",
    "fix-deps": "npm run clean:ultra && npm install --legacy-peer-deps",
    "deploy:clean": "npm run clean:ultra && npm install --legacy-peer-deps --production=false",
    "vercel-build": "npm ci --legacy-peer-deps && CI=false npm run build",
    "build:simple": "node build-simple.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "node": "22.x",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suggestly/elite-platform.git"
  },
  "keywords": [
    "ai",
    "uhnwi",
    "wealth-management",
    "luxury",
    "advisory",
    "react",
    "typescript"
  ],
  "author": "SUGGESTLY ELITE",
  "license": "MIT",
  "homepage": ".",
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^4.15.1"
  },
  "resolutions": {}
}
